<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="20"
            android:useLevel="false">
            <solid android:color="#1e6938" />
        </shape>
    </item>
    <item>
        <rotate>
            <shape
                android:shape="ring"
                android:thicknessRatio="20"
                android:useLevel="true">
                <gradient
                    android:startColor="#1e6938"
                    android:endColor="#1e6938"
                    android:type="linear" />
            </shape>
        </rotate>
    </item>
</layer-list>